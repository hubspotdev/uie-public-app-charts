{
  "name": "UIE Public App Card with Charts",
  "uid": "uie-public-app-charts",
  "description": "An example to demonstrate how to build a public app with developer projects, use the Charts component, and call data through hubspot.fetch.",
  "allowedUrls": ["https://api.hubapi.com", "https://softwium.com/api"],
  "auth": {
    "redirectUrls": ["http://localhost:3000/oauth-callback"],
    "requiredScopes": ["crm.objects.contacts.read", "crm.objects.contacts.write"],
    "optionalScopes": [],
    "conditionallyRequiredScopes": []
  },
  "support": {
    "supportEmail": "support@example.com",
    "documentationUrl": "https://example.com/docs",
    "supportUrl": "https://example.com/support",
    "supportPhone": "+18005555555"
  },
  "extensions": {
    "crm": {
      "cards": [
        {
          "file": "./extensions/charts-card.json"
        }
      ]
    }
  },
  "webhooks": {
    "file": "./webhooks/webhooks.json"
  }
}
